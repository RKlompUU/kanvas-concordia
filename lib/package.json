{
  "name": "kanvas_lib",
  "version": "0.0.1",
  "description": "shared library between admin-api and store-api",
  "author": "Rick Klomp",
  "license": "MIT",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "script/test"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.4",
    "@nestjs/core": "^8.4.4",
    "@nestjs/schedule": "^1.1.0",
    "@nestjs/testing": "^8.4.4",
    "@types/axios": "^0.14.0",
    "@types/cron": "^1.7.3",
    "@types/jest": "^27.4.1",
    "@types/luxon": "^2.3.1",
    "@types/node": "^17.0.29",
    "axios": "^0.27.2",
    "cron": "^1.8.2",
    "jest": "26.6.3",
    "luxon": "^2.3.2",
    "node": "^17.7.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "ts-jest": "26.5.6",
    "tsc": "^2.0.4",
    "typescript": "^4.6.3"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "test/coverage",
    "testEnvironment": "node"
  }
}
